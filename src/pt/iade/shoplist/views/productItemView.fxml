<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>


<HBox alignment="CENTER_LEFT" prefHeight="80.0" prefWidth="380.0" spacing="10.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/10.0.1">
   <children>
      <ImageView fx:id="imageIV" fitHeight="60.0" fitWidth="60.0" pickOnBounds="true" preserveRatio="true" />
      <VBox prefHeight="200.0" spacing="5.0" HBox.hgrow="ALWAYS">
         <children>
            <Label fx:id="nameL" text="Label" />
            <Label fx:id="quantityAndPriceL" text="Label" />
            <Label fx:id="totalL" text="Label" />
         </children>
         <padding>
            <Insets bottom="10.0" top="10.0" />
         </padding>
      </VBox>
   </children>
   <padding>
      <Insets left="10.0" />
   </padding>
</HBox>
